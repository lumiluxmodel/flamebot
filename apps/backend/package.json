{
  "name": "@flamebot/backend",
  "private": true,
  "version": "2.0.0",
  "main": "src/index.js",
  "scripts": {
    "dev": "nodemon src/index.js",
    "start": "node src/index.js",
    "build": "echo 'Backend build'",
    "lint": "eslint . --ext js --report-unused-disable-directives --max-warnings 0",
    "test:ai": "node test/testAI.js",
    "test:import": "node test/testImport.js",
    "test:swipe": "node test/testSwipe.js",
    "test:spectre": "node test/testSpectre.js",
    "test:bioprompt": "node test/testBioPrompt.js",
    "test:generation": "node test/testGenerationDirect.js",
    "test:special-models": "node test/testSpecialModels.js",
    "test:generation:bio": "node test/testGenerationDirect.js bio",
    "test:generation:prompt": "node test/testGenerationDirect.js prompt",
    "test:generation:all": "node test/testGenerationDirect.js -- --all",
    "test:all": "pnpm run test:ai && pnpm run test:import && pnpm run test:swipe && pnpm run test:spectre && pnpm run test:bioprompt",
    "test:models": "pnpm run test:special-models && pnpm run test:generation"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.3",
    "openai": "^5.3.0",
    "pg": "^8.16.0",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "eslint": "^8.55.0",
    "nodemon": "^3.0.2"
  }
}
